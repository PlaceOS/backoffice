{
    "index": "/index.html",
    "assetGroups": [
        {
            "name": "app",
            "installMode": "prefetch",
            "resources": {
                "files": [
                    "/favicon.ico",
                    "/index.html",
                    "/*.bundle.css",
                    "/*.bundle.js",
                    "/*.chunk.js"
                ]
            }
        },
        {
            "name": "assets",
            "installMode": "lazy",
            "updateMode": "prefetch",
            "resources": {
                "files": [
                    "/assets/**",
                    "!/assets/settings.json",
                    "!/assets/changelog.md"
                ],
                "urls": [
                    "https://fonts.googleapis.com/**",
                    "https://fonts.gstatic.com/**"
                ]
            }
        }
    ],
    "dataGroups": [
        {
            "name": "settings",
            "urls": [
                "/assets/settings.json",
                "/assets/changelog.md"
            ],
            "cacheConfig": {
                "strategy": "freshness",
                "maxSize": 10,
                "maxAge": "3h",
                "timeout": "10s"
            }
        },
        {
            "name": "control-api",
            "urls": [
                "/control",
                "/api",
                "/auth"
            ],
            "cacheConfig": {
                "strategy": "freshness",
                "maxSize": 100,
                "maxAge": "3d",
                "timeout": "10s"
            }
        }
    ]
}