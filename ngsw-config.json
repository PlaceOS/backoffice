{
    "$schema": "./node_modules/@angular/service-worker/config/schema.json",
    "index": "/index.html",
    "assetGroups": [
        {
            "name": "app",
            "installMode": "prefetch",
            "resources": {
                "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js",
          "/manifest.webmanifest"
        ]
            }
        },
        {
            "name": "assets",
            "installMode": "lazy",
            "updateMode": "prefetch",
            "resources": {
                "files": ["/assets/**", "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"]
            }
        },
        {
            "name": "fonts",
            "resources": {
                "urls": ["https://fonts.googleapis.com/**", "https://fonts.gstatic.com/**"]
            }
        }
    ],
    "dataGroups": [
        {
            "name": "settings",
            "urls": ["/assets/settings.json"],
            "cacheConfig": {
                "strategy": "freshness",
                "maxSize": 10,
                "maxAge": "3h",
                "timeout": "10s"
            }
        },
        {
            "name": "engine-api",
            "urls": ["/control", "/api"],
            "cacheConfig": {
                "strategy": "freshness",
                "maxSize": 100,
                "maxAge": "3d",
                "timeout": "10s"
            }
        }
    ]
}
